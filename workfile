    //Event.observe(window, 'load', function(e) {
    jQuery(document).ready(function() {
        var pageX = -1;
        var pageY = -1;
        if (pageX !== undefined && pageY !== undefined) {
            if (Number(pageX) > 0 || Number(pageY) > 0) {
                //$(window).scrollTo(pageX, pageY);
                window.scrollTo(Number(pageX), Number(pageY));
            }
        }
    });

    function setParam(url, name, value) {
        var regexp = new RegExp('[&\\?]' + name + '=' + '[^&#]*', 'i');
        var index = url.search(regexp);
        if (index != -1) {
            return url.replace(regexp, url.charAt(index) + name + '=' + value);
        } else {
            var urlchar = url.match(/\?/) ? '&' : '?';
            return url + urlchar + name + '=' + value;
        }
    }

    function reloadTab(index, sort, descending, tab, lotView, pageX, pageY) {
        jQuery.noConflict() (
            function($) {
                url = location.href.replace(/#[^#]*/, '').replace(location.pathname, '/pgz/public/action/search/simple/result');

                           url = setParam(url, 'index', index !== null ? index : '1');
                           url = setParam(url, 'sortField', sort !== undefined ? sort : 'lastEventDate');
                           url = setParam(url, 'descending', descending !== undefined ? descending : 'true');
                           url = setParam(url, 'tabName', tab !== undefined ? tab : 'FO');
                           url = setParam(url, 'lotView', lotView !== undefined ? lotView : 'false');
                               url = setParam(url, 'ext', '');
                               var query = url.substring(url.indexOf('?') + 1, url.length);
                               url = setParam(url, 'ext', kall(query));
                           url = setParam(url, 'pageX', pageX !== undefined ? pageX : '');
                           url = setParam(url, 'pageY', pageY !== undefined ? pageY : '');
                           location.replace(url);
                       }
            )
    }

    function paginateTab(index) {
        reloadTab(index);
    }

    function sortTab(sort, descending) {
        reloadTab(1, sort, descending);
    }

    function changeTab(tab) {
        reloadTab(1, 'lastEventDate', true, tab);
    }

    function changeLotView(lotView) {
        reloadTab(null, undefined, undefined, undefined, lotView, jQuery(document).scrollLeft(), jQuery(document).scrollTop());
    }

    function displayComissionStages(display) {
        reloadTab(null, undefined, undefined, undefined, display);
    }

    function exportResult() {

        if ((false && false && false)
                || (true && (false || false))
            ) {
            alert("В настоящее время функция выгрузки недоступна. Выгрузка результатов поиска осуществляется с 0:00 до 23:00 по московскому времени.");
            return false;
        }

        if (481633 > 500) {
            if (!confirm("Результаты поиска превышают допустимое количество записей для выгрузки. В случае продолжения операции в файл будут выгружены 500 реестровых записей (последних по дате обновления).")) {
                return false;
            }
        }

        url = location.href.replace(/#[^#]*/, '').replace(location.pathname, '/pgz/public/action/search/simple/resultExport');
        url = setParam(url, 'tabName', 'FO');
        url = setParam(url, 'lotView', 'false');
        location.replace(url);

    }


