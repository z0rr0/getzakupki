                                                        <span id="initiatorIdParam" style="display:none;"></span><span id="initiatorFullNameParam" style="display:none;"></span><script type="text/javascript">
    //

        function onMorphologySearch() {
            var strictCheck = document.getElementById("orderNameStrictCheckId");
            var morphologyCheck = document.getElementById("orderNameMorphologyCheckId");
            strictCheck.disabled = morphologyCheck.checked;

            var morphActiveTooltip = document.getElementById("morphActiveTooltip");
            var morphUnactiveTooltip = document.getElementById("morphUnactiveTooltip");
            var morphInitialTooltip = document.getElementById("morphInitialTooltip");

            if (morphologyCheck.checked) {
                morphActiveTooltip.style.display = "inline";
                morphUnactiveTooltip.style.display = "none";
                morphInitialTooltip.style.display = "none";
                strictCheck.checked = false;

            } else {
                morphActiveTooltip.style.display = "none";
                morphUnactiveTooltip.style.display = "inline";
                morphInitialTooltip.style.display = "none";
            }
        }

        function getInitiatorFullNameParam() {
            return getEl("initiatorFullNameParam").innerHTML;
        }

        function getInitiatorIdParam() {
            return getEl("initiatorIdParam").innerHTML;
        }

        function getInitiatorIdInput() {
            return getEl("initiatorIdInputId");
        }

        function getInitiatorNameInput() {
            return getEl("initiatorFullNameInputId");
        }

        function getInitiatorClearLink() {
            return getEl("initiatorClearLink");
        }

        function checkInitiatorClearLink() {
            getInitiatorClearLink().style.display = getInitiatorNameInput().readOnly ? "block" : "none";
        }

        function clearInitiator() {
            getInitiatorNameInput().value = "";
            getInitiatorNameInput().readOnly = false;
            getInitiatorNameInput().style.backgroundColor =  "white";
            getInitiatorIdInput().value = "";
            checkInitiatorClearLink();
        }

        function selectOrganizationCallback(organizationFullname, orgId) {
            if (isEmpty(organizationFullname) || isEmpty(orgId)) {
                return;
            }
            selectValue_initiatorFullNameInputId(organizationFullname);

            getInitiatorIdInput().value = orgId;

            getInitiatorNameInput().style.fontStyle = 'normal';
            getInitiatorNameInput().style.backgroundColor =  "#d3d3d3";
            getInitiatorNameInput().readOnly = true;
            checkInitiatorClearLink();
        }

        function openWindow(url, name, width, height) {
            var leftVal = (screen.width - width) / 2;
            var topVal = (screen.height - height) / 2;
            var newWindow = window.open(appRoot + url, name, "width=" + width + ", height=" + height + ", left=" + leftVal + ", top=" + topVal + ", scrollbars=1, resizable=1");
            if (newWindow !== null) {
                newWindow.focus();
            }
        }

        function onStrictSearch() {
            var strictCheck = document.getElementById("orderNameStrictCheckId");
            var morphologyCheck = document.getElementById("orderNameMorphologyCheckId");
            morphologyCheck.disabled = strictCheck.checked;
            if (strictCheck.checked) {
                morphologyCheck.checked = false;
            }
        }
