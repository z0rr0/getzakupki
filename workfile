        function doSearch() {
            var searchValue = jQuery.trim(jQuery('#quickSearchInput').val());
            return searchValue !== undefined && searchValue !== '';
        }

	    function doCheck() {
	      	jQuery.noConflict()(
	      		function($) {
		            if ($('#useMorphologyCheckboxId:checked').val() !== undefined) {
		        		 $("#morphologyActive").show();
		        		 $("#morphologyDisactive").hide();
		        	}
		        	else {
			       		 $("#morphologyActive").hide();
		    			 $("#morphologyDisactive").show();
		        	}
	      		}
	    	)
	    }

