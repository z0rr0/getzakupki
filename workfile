//

function viewOrderInfo(action) {
    jQuery.noConflict()(
            function($) {
                var newLocation = '/pgz/public/action/orders/info/' + action;
                saveCookie(PublicCookies.SEARCH_URL, window.location);
                saveCookie(PublicCookies.TAB_NAME, 'FO');
                window.open(newLocation);
            })
}

function collapserHanler(collapsed) {
	jQuery.noConflict()(
			function($) {
				if (collapsed) {
					$(".collapsiblePanel").css("display", "none");
					$(".expandedCollapser").addClass("collapsedCollapser");
					$(".expandedCollapser").removeClass("expandedCollapser");
				} else {
					$(".collapsiblePanel").css("display", "block");
					$(".collapsedCollapser").addClass("expandedCollapser");
					$(".collapsedCollapser").removeClass("collapsedCollapser");
				}
			}
			)
}


    function get_ww()
    {
        var frameWidth=800;
        if (self.innerWidth)
            frameWidth = self.innerWidth;
        else if (document.documentElement && document.documentElement.clientWidth)
            frameWidth = document.documentElement.clientWidth;
        else if (document.body)
            frameWidth = document.body.clientWidth;
        return frameWidth;
    }
    function get_wh()
    {
        var frameHeight=640;
        if (self.innerHeight)
            frameHeight = self.innerHeight;
        else if (document.documentElement && document.documentElement.clientHeight)
            frameHeight = document.documentElement.clientHeight;
        else if (document.body)
            frameHeight = document.body.clientHeight;
        return frameHeight;
    }

    function setVisible(id)
    {
        obj = document.getElementById(id);
        obj.style.visibility = 'visible';
        placeIt(id);
    }

    function closeAdditionalWindow(id){
        obj = document.getElementById(id);
        obj.style.visibility = 'hidden';
    }

    function placeIt(id)
    {
        //alert(id);
        theLeft = get_ww()/2 - 250;
        theTop = get_wh()/2 - 100;
        obj = document.getElementById(id);
        obj.style.left = theLeft + 'px' ;
        obj.style.top = theTop + 'px' ;
        window.setTimeout('placeIt("' + id + '");', 1000);
    }



